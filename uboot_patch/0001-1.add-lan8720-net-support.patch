+<!DOCTYPE HTML>
+<html>
+<head>
+<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
+<title></title>
+<meta name="keywords" content=""/>
+<meta name="description" content=""/>
+<link href="style.css" rel="stylesheet"/>
+<script src="jquip.min.js"></script>
+<script type="text/javascript">
+	$(function(){
+		if(window.screen.availHeight>=870){
+			var marginTop=(window.screen.availHeight*0.15)+"px";
+		}else if(window.screen.availHeight>=738){
+			var marginTop=(window.screen.availHeight*0.095)+"px";
+		}else if(window.screen.availHeight<=690){
+			var marginTop=(window.screen.availHeight*0.06)+"px";
+		}
+		$('.logo_01').css('marginTop',marginTop);
+	});
+	function check()
+	{
+		var user = window.document.getElementById("user").value;
+		var psk = window.document.getElementById("psk").value;
+		if (user == "")
+		{
+			alert("");
+		}
+		else if (psk == "")
+		{
+			alert("");
+		}
+		else
+		{
+			$.ajax({ 
+			url: "/auth-bin/login?user="+user+"&psk="+psk+"&__time="+Date.parse( new Date()), 
+			async: true,
+			cache: false,
+			dataType: 'json', 
+			success: function(data){
+				if(data.ret==1){
+					self.location = 'index.html';
+				} else {
+					alert(data.msg);
+				}
+			},
+			timeout: 5000,
+			error: function( ){
+				alert( ",!" );
+			}
+			});
+		}
+	}
+</script>
+</head>
+<body>
+	<div class="logo_01">
+			<img src="logo2.png">
+	</div>
+	<div class="logo_02">
+		uboot
+	</div>
+	<div id="Logo">
+		<form class="form" name="form1" method="post" action="">
+			<div class="form_box">
+				<p><img src="1.png"/><input onkeydown='if(event.keyCode==13){login.click()}' type="text" name="user" id="user" placeholder=""></p>
+				<p><img src="2.png"/><input onkeydown='if(event.keyCode==13){login.click()}' type="password" name="psk" id="psk" placeholder=""></p>
+				<p class="che"><input id="checkbox1" type="checkbox" value="">&nbsp;&nbsp;</p>
+			</div>
+			<div class="form_submit">
+				<a href="#" id="login" onclick="check();"></a>
+			</div>
+		</form>
+		<div class="footbox">Copyright  2016  All rights reserved.</div>
+	</div>
+</body>
